package textExcel;

//Update this file with your own code.

public class SpreadsheetLocation implements Location
{
    
	private int rows = 20;
	private int cols = 12;

    public int getRow()
    {
        return rows-1;
    }

    @Override
    public int getCol()
    {
        
        return cols;
    }
    
    public SpreadsheetLocation(String cellName)
    {
    	int rowName = Integer.parseInt(cellName.substring(1));
    	int column = cellName.charAt(0) - 'A';
    	System.out.print(rowName + column);
    }

}
