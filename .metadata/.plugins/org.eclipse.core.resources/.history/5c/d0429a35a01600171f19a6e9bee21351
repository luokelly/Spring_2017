package textExcel;

public class TextCell implements Cell {
	
	private String text;
	
	public TextCell (String input){
		this.text = input;
	}
	@Override
	public String abbreviatedCellText() {
		// TODO Auto-generated method stub
		String abrv = text;
		if(abrv.length() < 10){
			while(abrv.length() < 10){
				abrv += " ";
			}
			return abrv;
		} else if(abrv.length() > 10){
			abrv = abrv.substring(0, 10);
			return abrv;
		}else{
			return abrv;
		}
	}

	@Override
	public String fullCellText() {
		// TODO Auto-generated method stub
		return "\"" + text + "\"";
	}
	public String 
}